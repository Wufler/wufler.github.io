		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"lucide-react": "^0.511.0",
		"motion": "^12.12.1",
		"next": "15.3.2",
		"next-themes": "^0.4.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"sonner": "^2.0.3",
		"tailwind-merge": "^3.3.0",
		"tailwindcss-animate": "^1.0.7",
		"ts-node": "^10.9.2"
	},
	"devDependencies": {
